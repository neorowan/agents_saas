# Development Environment Variables
# Copy this file to .env.local for local development

# Database
DATABASE_URL=postgresql://postgres:lobe_password_123@localhost:5432/lobe_chat_dev
DATABASE_DRIVER=node
POSTGRES_PASSWORD=lobe_password_123
LOBE_DB_NAME=lobe_chat_dev

# Redis
REDIS_URL=redis://localhost:6379

# MinIO/S3
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin123
S3_BUCKET=lobe-files
S3_PUBLIC_DOMAIN=http://localhost:9000
NEXT_PUBLIC_S3_DOMAIN=http://localhost:9000

# Casdoor Auth
AUTH_CASDOOR_ISSUER=http://localhost:8000
AUTH_CASDOOR_ID=your-client-id-here
AUTH_CASDOOR_SECRET=your-client-secret-here
NEXT_AUTH_SSO_PROVIDERS=casdoor

# Security Keys
NEXT_AUTH_SECRET=generate-a-secure-secret-here
KEY_VAULTS_SECRET=generate-another-secure-secret-here
NEXTAUTH_URL=http://localhost:3010

# Application
APP_URL=http://localhost:3010
NEXT_PUBLIC_SITE_URL=http://localhost:3010
NEXT_PUBLIC_SERVICE_MODE=server
NEXT_PUBLIC_ENABLE_NEXT_AUTH=1
NEXT_PUBLIC_CSP_DISABLED=true

# Development
NODE_ENV=development
PORT=3010
DEBUG=true

