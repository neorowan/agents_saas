# ==================================================
# CONFIGURAÇÕES DO BANCO DE DADOS
# ==================================================
POSTGRES_PASSWORD=agents_chat_password_123
LOBE_DB_NAME=agents_chat_prod

# URL completa do banco (IMPORTANTE!)
DATABASE_URL=postgresql://postgres:agents_chat_password_123@postgresql:5432/agents_chat_prod
DATABASE_DRIVER=node

# ==================================================
# CONFIGURAÇÕES DO MINIO (S3 LOCAL)
# ==================================================
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minio_password_123
MINIO_PORT=9000
MINIO_LOBE_BUCKET=agents-chat-files

# Configurações S3 para o Lobe Chat
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minio_password_123
S3_ENDPOINT=http://minio:9000
S3_BUCKET=agents-chat-files
S3_PUBLIC_DOMAIN=http://161.35.227.30:9000
NEXT_PUBLIC_S3_DOMAIN=http://161.35.227.30:9000

# ==================================================
# CONFIGURAÇÕES DO CASDOOR (AUTENTICAÇÃO)
# ==================================================
CASDOOR_PORT=8000

# URLs do Casdoor
AUTH_CASDOOR_ISSUER=http://161.35.227.30:8000
NEXTAUTH_URL=http://161.35.227.30:3210

# Credenciais do Casdoor (CONFIGURE NO CASDOOR!)
AUTH_CASDOOR_ID=your-casdoor-client-id
AUTH_CASDOOR_SECRET=your-casdoor-client-secret
AUTH_CASDOOR_ENDPOINT=http://casdoor:8000
AUTH_CASDOOR_PUBLIC_ENDPOINT=http://161.35.227.30:8000

# Configurações adicionais do Casdoor
AUTH_CASDOOR_ORGANIZATION=built-in
AUTH_CASDOOR_APPLICATION=app-built-in
AUTH_CASDOOR_CERTIFICATE=-----BEGIN CERTIFICATE-----
MIIE...seu-certificado-casdoor-aqui
-----END CERTIFICATE-----

# ==================================================
# CONFIGURAÇÕES DA APLICAÇÃO
# ==================================================
LOBE_PORT=3210
NEXT_PUBLIC_SITE_URL=http://161.35.227.30:3210
APP_URL=http://161.35.227.30:3210
HOSTNAME=0.0.0.0
PORT=3210

# ==================================================
# CONFIGURAÇÕES DE SEGURANÇA
# ==================================================
NEXT_AUTH_SECRET=your-secret-key-here-123-change-this-to-random-32-chars
KEY_VAULTS_SECRET=your-key-vault-secret-123-change-this-to-random-32-chars

# ==================================================
# CONFIGURAÇÕES DO NEXT AUTH
# ==================================================
NEXT_PUBLIC_ENABLE_NEXT_AUTH=1
NEXT_PUBLIC_SERVICE_MODE=server
NEXT_AUTH_SSO_PROVIDERS=casdoor

# ==================================================
# API KEYS DOS MODELOS (configure as que usar)
# ==================================================
# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL_LIST=gpt-3.5-turbo,gpt-4

# Anthropic
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL_LIST=claude-3-haiku,claude-3-sonnet

# Google
GOOGLE_API_KEY=
GOOGLE_MODEL_LIST=gemini-pro

# Azure OpenAI
AZURE_API_KEY=
AZURE_ENDPOINT=
AZURE_API_VERSION=2024-02-15-preview
AZURE_MODEL_LIST=

# ==================================================
# CONFIGURAÇÕES OPCIONAIS
# ==================================================
ACCESS_CODE=
DEBUG=false
FEATURE_FLAGS=

# Limite de upload (em MB)
NEXT_PUBLIC_UPLOAD_MAX_SIZE=50

# Workers do Next.js
NEXT_TELEMETRY_DISABLED=1

# Proxy (se necessário)
PROXY_URL=

# ==================================================
# CONFIGURAÇÕES DE PERFORMANCE (8GB RAM)
# ==================================================
NODE_OPTIONS=--max-old-space-size=2048 --optimize-for-size --gc-interval=100
NODE_ENV=production

# ==================================================
# LOGS E MONITORAMENTO (opcional)
# ==================================================
# Sentry
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=

# Analytics
NEXT_PUBLIC_ANALYTICS_POSTHOG=false
NEXT_PUBLIC_ANALYTICS_UMAMI=false